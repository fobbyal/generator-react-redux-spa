{
  "name": "<%= name %>",
  "version": "0.0.1",
  "description": "<%= desc %>",
  "main": "src/app.js",
  "scripts": {
    "prebuild:prod": "npm run clean-dist",
    "build:prod": "webpack --env.prod -p",
    "prebuild": "npm run clean-dist",
    "build": "webpack --env.dev",
    "start": "npm-run-all --parallel start:*",
    "start:client": "node dev-server.js",
    "start:mock-api": "node mock-api/api.js",
    "train": "npm-run-all --parallel train:*",
    "train:client": "node dev-server.js train.integdev.com",
    "train:mock-api": "node mock-api/api.js -h train.integdev.com",
    "clean-dist": "rimraf dist",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "lint:fix-file": "eslint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cover": "jest --coverage",
    "storybook": "start-storybook -p 6006",
    "analyze": "webpack --env.prod --env.ana -p --profile --json > stats.json",
    "pretty": "prettier --single-quote --trailing-comma es5 --no-semi --debug-check \"src/**/*.js\"",
    "pretty:write": "prettier --single-quote --trailing-comma es5 --no-semi --write \"src/**/*.js\"",
    "precommit": "lint-staged",
    "build-storybook": "build-storybook"
  },
  "author": "<%= author %>",
  "license": "ISC",
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.13",
    "@storybook/addon-links": "^3.2.13",
    "@storybook/react": "^3.2.13",
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.17.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^2.2.0",
    "babel-plugin-ramda": "^1.4.3",
    "babel-polyfill": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "body-parser": "^1.15.2",
    "chance": "^1.0.12",
    "cors": "^2.8.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "eslint": "^4.9.0",
    "eslint-import-resolver-babel-module": "3.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.4.0",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "fetch-mock": "^5.13.1",
    "file-loader": "^1.1.5",
    "git-rev-sync": "^1.9.1",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "jsonfile": "^4.0.0",
    "lint-staged": "^4.3.0",
    "npm-run-all": "^4.1.1",
    "postcss-loader": "^2.0.8",
    "prettier": "^1.7.4",
    "progress-bar-webpack-plugin": "^1.10.0",
    "react-addons-test-utils": "^15.4.1",
    "react-hot-loader": "^3.1.1",
    "react-test-renderer": "^16.0.0",
    "redbox-react": "^1.3.3",
    "redux-mock-store": "^1.2.1",
    "rimraf": "^2.5.4",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "uuid": "^3.0.1",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dashboard": "^1.0.0",
    "webpack-dev-server": "^2.9.3",
    "yargs": "^10.0.3"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "data.either": "^1.5.0",
    "data.maybe": "^1.2.2",
    "data.task": "^3.1.1",
    "debounce": "^1.0.0",
    "futurize": "^1.2.0",
    "immutable": "^3.8.2",
    "immutable-ext": "^1.0.8",
    "moment": "^2.19.1",
    "normalize.css": "^7.0.0",
    "numeral": "^2.0.1",
    "prop-types": "^15.6.0",
    "ramda": "^0.25.0",
    "react": "^16.0.0",
    "react-addons-shallow-compare": "^15.4.1",
    "react-contextmenu": "^2.8.0",
    "react-dates": "^12.1.2",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-virtualized": "^9.12.0",
    "recompose": "^0.26.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "semantic-ui-react": "^0.75.1",
    "styled-components": "^2.2.2",
    "whatwg-fetch": "^2.0.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "setupFiles": [
      "./mock-api/mock-api-for-tests.js"
    ]
  },
  "lint-staged": {
    "*.{js,json,css}": [
      "prettier --single-quote --trailing-comma es5 --no-semi --write",
      "git add"
    ]
  }
}
